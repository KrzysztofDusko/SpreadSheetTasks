<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
	  <TargetFrameworks>netstandard2.1</TargetFrameworks>
    <RootNamespace>SpreadSheetTasks</RootNamespace>
    <Authors>Krzysztof Duśko</Authors>
    <Description>The .NET library for fast reading and writing Excel files (.xlsx, .xlsb)</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
    <PackageReleaseNotes>Performance improvements, Csv writer preview</PackageReleaseNotes>
    <PackageTags>xlsx;xlsb;excel;spreadsheets</PackageTags>
    <Company>Krzysztof Duśko</Company>
    <Version>0.0.4</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/KrzysztofDusko/SpreadSheetTasks</RepositoryUrl>
    <PackageProjectUrl>https://github.com/KrzysztofDusko/SpreadSheetTasks</PackageProjectUrl>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
  </PropertyGroup>
</Project>
